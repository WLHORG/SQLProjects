    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@P_Region">
            <Value>=Parameters!P_Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
		
DECLARE @Start int
DECLARE @Threshold int 
DECLARE @Region varchar(10)
SET @Threshold = 500
SET @Start = CASE WHEN  DATEPART(weekday,GetDate()) = 2  THEN 3 ELSE 1 END
SET @Region = @P_Region


SELECT 	  COALESCE(NULLIF(c.CISCOntactNumber, 0), NULLIF(o.CISCOntactNumber, 0)) as contact_number
		, CASE WHEN FK_Contact = 1 THEN 'O' ELSE 'C' END as Contact_Type
		, COALESCE(NULLIF(c.label_name, ''), NULLIF(o.label_name, 'N/A')) as label_name
		, s.source
		, ISNULL(NULLIF(r.rf_area, ''), 'CENTRAL') as rf_area
		, ISNULL(NULLIF(r.rf_area_desc, ''), 'CENTRAL') as rf_area_desc
		, CONVERT(DATE, d.[date], 101) as transaction_date
		, ISNULL(NULLIF(t.transaction_origin_desc, 'N/A'), 'Other') as transaction_origin_desc
		, a.cost_centre_activity
		, a.cost_centre_activity_desc
		, f.Amount
		FROM [dbo].[Fundraising] f
		INNER JOIN [dbo].[DimContact] c ON f.FK_Contact = c.PK_ID
		INNER JOIN [dbo].[DimOrganisation] o ON f.FK_Organisation = o.PK_ID
		INNER JOIN [dbo].[DimRegion] r  ON f.FK_ContactRegion = r.PK_ID
		INNER JOIN [dbo].[DimSource] s ON f.FK_Source = s.PK_ID
		INNER JOIN [dbo].[DimActivity] a ON f.FK_Activity = a.PK_ID
		INNER JOIN [dbo].[DimDate] d ON f.FK_Date = d.Datekey
		INNER JOIN [dbo].[DimTranOrigin] t ON f.FK_TranOrigin = t.PK_ID
	WHERE FK_DATE BETWEEN CONVERT(VARCHAR(8),GETDATE(),112) - @Start and CONVERT(VARCHAR(8),GETDATE(),112)  

AND f.Amount &gt;= @Threshold
		AND   (F.FK_Team = 10) 
		AND (rf_area  = @Region  or @Region  IS NULL)</CommandText>
      </Query>
