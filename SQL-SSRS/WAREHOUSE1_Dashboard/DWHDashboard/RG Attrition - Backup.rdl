  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ActiveCNT">
          <DataField>ActiveCNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ATTCNT">
          <DataField>ATTCNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NOSHOWCNT">
          <DataField>NOSHOWCNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="channel_desc">
          <DataField>channel_desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT COUNT(CASE WHEN  sub.last_payment_date BETWEEN GETDATE() - 31 and GETDATE() THEN 1 ELSE NULL END) as ActiveCNT,
COUNT(CASE  WHEN sub.cancelled_on IS NOT NULL THEN 2 WHEN sub.last_payment_date &gt; GETDATE()- 31 THEN 2 ELSE NULL END) as ATTCNT, 
COUNT(CASE  WHEN sub.ordPK is NULL then 1  ELSE NULL END) as NOSHOWCNT, 
sub.channel_desc
FROM 
(
SELECT DISTINCT o.order_number, c.Channel_desc, cancellation_reason, cancelled_on, payment_number, last_payment_date, o.PK_ID as OrdPK
FROM DimOrder o
LEFT OUTER JOIN fundraising f  ON  o.PK_ID = f.FK_Orders
LEFT OUTER JOIN DimDate d ON  d.Datekey = f.FK_Date 
LEFT OUTER JOIN  DimContact cn ON f.FK_Contact = cn.pk_id
LEFT OUTER JOIN DimChannel c ON f.FK_Channel = c.PK_ID
LEFT OUTER JOIN DimSource s ON f.fk_source = s.PK_ID
LEFT OUTER JOIN DimProducts p ON f.FK_Product = p.pk_id
WHERE product ='DON-REGGIV-DUK-OS' and (o.payment_frequency in ('M', 'I') and o.last_payment_date&gt;dateadd(mm,-2,getdate())
		or (o.payment_frequency in ('Q', 'J') and o.last_payment_date&gt;dateadd(mm,-4,getdate()))
		or (o.payment_frequency in ('A', 'K') and o.last_payment_date&gt;dateadd(mm,-13,getdate()))
	and o.cancelled_on is null) 
AND o.order_date BETWEEN GETDATE() - 365 and GETDATE()
) sub
GROUP BY sub.channel_desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
