    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>if object_id('tempdb..#CORPAM_Actions') is not null drop table #CORPAM_Actions
if object_id('tempdb..#Related_Actions') is not null drop table #Related_Actions
if object_id('tempdb..#mergedactiontable') is not null drop table #mergedactiontable

--create table listing actions where account managers are associated as actioners
CREATE table #CORPAM_Actions
(action_number int
,actioner_number int
,actioner_name varchar(80))

insert into #CORPAM_Actions
select ca.action_number
,ca.contact_number
,c.label_name
from cis.contact_actions ca
inner join cis.contacts c
ON ca.contact_number = c.contact_number
INNER JOIN cis.actions a
ON a.action_number = ca.action_number
where (ca.contact_number IN ('1674694','1850399','1821775') 
AND ca.type = 'A')
--AND a.action_priority IN ('TMDN', 'TMDF', 'TMDG')
AND a.completed_on IS NULL
UNION
select ca.action_number
,ca.contact_number
,c.label_name
from cis.contact_actions ca
left join cis.contact_categories cc
on ca.contact_number = cc.contact_number
left join cis.contacts c
on ca.contact_number = c.contact_number
where (cc.activity = 'CORPAM'
and cc.activity_value IN ('MP','BE','EHO')
and cc.valid_to &gt;= GETDATE())
and ca.type = 'A'
--select * from #CORPAM_Actions
--order by #CORPAM_Actions.action_number

--create table listing related to contacts for CORPAM actioners table
CREATE table #Related_Actions
(action_number int
,related_number int
,related_name varchar(80))

insert into #Related_Actions
select #CORPAM_Actions.action_number
,ca.contact_number
,c.label_name
from cis.contact_actions ca
LEFT JOIN #CORPAM_Actions
ON #CORPAM_Actions.action_number = ca.action_number
inner join cis.contacts c
ON ca.contact_number = c.contact_number
INNER JOIN cis.actions a
ON ca.action_number = a.action_number
where ca.type = 'R'
--AND a.action_priority IN ('TMDN', 'TMDF', 'TMDG')
AND a.completed_on IS NULL
UNION
select #CORPAM_Actions.action_number
,ca.contact_number
,c.name
from cis.contact_actions ca
LEFT JOIN #CORPAM_Actions
ON #CORPAM_Actions.action_number = ca.action_number
inner join cis.organisations c
ON ca.contact_number = c.contact_number
INNER JOIN cis.actions a
ON ca.action_number = a.action_number
where ca.type = 'R'
--AND a.action_priority IN ('TMDN', 'TMDF', 'TMDG')
AND a.completed_on IS NULL;
--select * from #Related_Actions
--INNER JOIN #CORPAM_Actions
--ON #Related_Actions.action_number = #CORPAM_Actions.action_number
--ORDER BY #Related_Actions.action_number

--merge temp tables to create one containing actioner and related to contact and drop original temp tables
create table #mergedactiontable
(action_number int
,related_to_contact_number int
,related_to_name varchar(80)
,actioner_number int
,actioner_name varchar(80))

insert into #mergedactiontable
select #CORPAM_Actions.action_number
,#Related_Actions.related_number
,#Related_Actions.related_name
,#CORPAM_Actions.actioner_number
,#CORPAM_Actions.actioner_name
FROM #CORPAM_Actions
LEFT JOIN #Related_Actions
ON #CORPAM_Actions.action_number = #Related_Actions.action_number
drop table #CORPAM_Actions, #Related_Actions;
--select * from #mergedactiontable

select
#mergedactiontable.related_to_contact_number
,#mergedactiontable.related_to_name
,c.notes
,#mergedactiontable.actioner_name
,#mergedactiontable.action_number
,a.action_desc
,convert(varchar(10),a.scheduled_on,103) Scheduled_On
,convert(varchar(10),a.deadline,103)Action_Deadline
,fr.request_description Fundraising_Request_Description
,frs.fundraising_request_stage_desc FR_Stage
,frt.fundraising_request_type_desc FR_Type
,fs.fundraising_status_desc FR_Status
,fr.target_amount FR_Target
,convert(varchar(10),fr.target_date,103) FR_Target_Date
from #mergedactiontable
LEFT join cis.actions a
on #mergedactiontable.action_number = a.action_number
LEFT join cis.fundraising_requests fr
on #mergedactiontable.related_to_contact_number = fr.contact_number
LEFT JOIN cis.fundraising_request_stages frs
on fr.fundraising_request_stage = frs.fundraising_request_stage
LEFT JOIN cis.fundraising_request_types frt
on fr.fundraising_request_type = frt.fundraising_request_type
LEFT JOIN cis.fundraising_statuses fs
on fr.fundraising_status = fs.fundraising_status
LEFT JOIN cis.contacts c
on #mergedactiontable.related_to_contact_number = c.contact_number
where (fr.fundraising_request_stage != 'RC'
OR fr.fundraising_request_stage IS NULL)

AND datepart(yyyy,a.scheduled_on) = DATEPART (yyyy,getdate())

AND a.action_status NOT IN ('COMP', 'CANC')
ORDER BY a.scheduled_on ASC</CommandText>
