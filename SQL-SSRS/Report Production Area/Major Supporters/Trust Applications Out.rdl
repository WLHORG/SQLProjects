<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Lists all trust applications with a target date in the current year that are currently waiting on a donor response.
Data Source: DiabetesDWH</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>/Data Sources/SharedDataSource_DiabetesDWH</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>66dc46a5-8a5f-42be-b3cd-64b673190e51</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
o.organisation_number
,o.name
,fr.request_description
,frs.fundraising_request_stage_desc
,frt.fundraising_request_type_desc
,fs.fundraising_status_desc
,fr.target_amount
,convert(varchar(10),fr.target_date,103) Target_Date
from cis.fundraising_requests fr
inner join cis.organisations o
on fr.contact_number = o.organisation_number
Inner join cis.organisation_categories oc
on fr.contact_number = oc.organisation_number
LEFT JOIN cis.fundraising_request_stages frs
on fr.fundraising_request_stage = frs.fundraising_request_stage
LEFT JOIN cis.fundraising_request_types frt
on fr.fundraising_request_type = frt.fundraising_request_type
LEFT JOIN cis.fundraising_statuses fs
on fr.fundraising_status = fs.fundraising_status
where (oc.activity = 'CORPAM'
AND oc.activity_value IN ('KL','BB','OJ','LSM','AW','AHA') -- GG 19-11-2015
AND oc.valid_to &gt;= GETDATE())
AND fr.fundraising_request_number IS NOT NULL
AND datepart(yyyy,fr.target_date) = DATEPART(YYYY,GETDATE())
AND fr.fundraising_status = 'WAIT'
ORDER BY fr.target_date ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
