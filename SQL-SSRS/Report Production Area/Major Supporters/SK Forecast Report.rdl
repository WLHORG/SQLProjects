---------------------------------------------------------------------------------------------------

--if object_id('tempdb..#currentyear') is not null drop table #currentyear
if OBJECT_ID('tempdb..#CORPAM3300') is not null drop table #CORPAM3300
if OBJECT_ID('tempdb..#FR') is not null drop table #FR
if OBJECT_ID('tempdb..#banked') is not null drop table #banked
if OBJECT_ID('tempdb..#MG') is not null drop table #MG
if OBJECT_ID('tempdb..#GA') is not null drop table #GA
if OBJECT_ID('tempdb..#MSP') is not null drop table #MSP

create table #CORPAM3300
(contact_number int
,Name varchar (80))

INSERT into #CORPAM3300 
select
fr.contact_number
,case
when c.forenames IS NULL then c.label_name
else (c.title + ' ' + c.preferred_forename + ' ' + c.surname)
end
from cis.fundraising_requests fr
left join cis.contacts c
on fr.contact_number = c.contact_number
INNER JOIN cis.contact_categories cc
on c.contact_number = cc.contact_number
where (fr.fundraising_request_number IS NOT NULL
	AND DATEPART(yyyy, fr.request_date) = DATEPART (yyyy, getdate()))
AND		(cc.activity = 'CORPAM'
	AND cc.activity_value IN ('BE', 'MP', 'EHO')
	AND cc.valid_to &gt;= getdate())
UNION
SELECT
	c.contact_number
	,case
when c.forenames IS NULL then c.label_name
else (c.title + ' ' + c.preferred_forename + ' ' + c.surname)
end
FROM cis.contacts c
INNER JOIN wft.wft_financial_history_merge fh
ON c.contact_number = fh.contact_number
INNER JOIN cis.rates r
on fh.product = r.product
and fh.rate = r.rate
LEFT JOIN cis.duk_new2015_products dnp on dnp.product = r.product
LEFT JOIN cis.duk_new2015_sources dnsr on dnsr.source = fh.source and dnsr.new_product is null and dnsr.region = r.nominal_account_suffix
LEFT JOIN cis.duk_new2015_sources dnsp on dnsp.source = fh.source and dnp.new_product is not null and dnsp.new_product = dnp.new_product and dnsp.region is null
LEFT JOIN cis.duk_new2015_sources dns on dns.source = fh.source and dns.new_product is null and dns.region is null
WHERE (dnsr.new_cost_centre in ('FMMD','FCWS')
or dnsp.new_cost_centre in ('FMMD','FCWS')
or dns.new_cost_centre in ('FMMD','FCWS'))  
and (substring(dnsr.new_source,6,1) = 'X'
or substring(dnsp.new_source,6,1) = 'X'
or substring(dns.new_source,6,1) = 'X')
AND DATEPART(yyyy, fh.transaction_date) = datepart (yyyy, getdate())
and fh.invoice = 0

create table #FR
(contact_number int
,Target_Amount numeric (9,2)
,Target_Month varchar(10)
,Target_Year varchar(10)
,target_date datetime
,Pledged_Amount  numeric (9,2)
,Indicated_Amount numeric (9,2)
,Identified_Amount numeric (9,2)
,Project varchar (30)
,Stage varchar(4))

INSERT INTO #FR
select C3.contact_number
,ISNULL (fr.target_amount,0)
,Expected_Month = datename(MONTH, fr.target_date)
,Expected_Year = DATEname (YYYY, fr.target_date)
,fr.target_date
,Pledged_amount = ISNULL (fr.pledged_amount,0)
,Indicated_Amount = ISNULL (fr.gik_expected_amount,0)
,Identified_Amount = ISNULL (fr.expected_amount,0)
,frt.fundraising_request_type_desc
,fr.fundraising_request_stage
from #CORPAM3300 C3
LEFT JOIN cis.fundraising_requests fr
on C3.contact_number = fr.contact_number
INNER JOIN cis.fundraising_request_types frt
on fr.fundraising_request_type = frt.fundraising_request_type
WHERE DATEPART (yyyy, fr.target_date) = DATEPART (YYYY, getdate())
ORDER BY C3.contact_number ASC

create table #banked
(contact_number int
,Banked numeric (9,2))

insert into #banked
select
C3.contact_number
,ISNULL(SUM(fh.amount),0)
from #CORPAM3300 C3
INNER JOIN wft.wft_financial_history_merge fh
ON C3.contact_number = fh.contact_number
INNER JOIN cis.rates r
on fh.product = r.product
and fh.rate = r.rate
LEFT JOIN cis.duk_new2015_products dnp on dnp.product = r.product
LEFT JOIN cis.duk_new2015_sources dnsr on dnsr.source = fh.source and dnsr.new_product is null and dnsr.region = r.nominal_account_suffix
LEFT JOIN cis.duk_new2015_sources dnsp on dnsp.source = fh.source and dnp.new_product is not null and dnsp.new_product = dnp.new_product and dnsp.region is null
LEFT JOIN cis.duk_new2015_sources dns on dns.source = fh.source and dns.new_product is null and dns.region is null
WHERE datepart(YYYY, fh.transaction_date) = datepart (YYYY, getdate())
and (dnsr.new_cost_centre in ('FMMD','FCWS')
or dnsp.new_cost_centre in ('FMMD','FCWS')
or dns.new_cost_centre in ('FMMD','FCWS'))  
and (substring(dnsr.new_source,6,1) = 'X'
or substring(dnsp.new_source,6,1) = 'X'
or substring(dns.new_source,6,1) = 'X')
and fh.invoice = 0
GROUP BY C3.contact_number


create table #MG
(contact_number int
,activity_desc varchar (40))

insert into #MG
select
C3.contact_number
,a.activity_desc
from #CORPAM3300 C3
INNER JOIN cis.contact_categories cc
ON C3.contact_number = cc.contact_number
INNER JOIN cis.activities a
on cc.activity = a.activity
WHERE cc.activity = 'MG'
AND cc.valid_to &gt;= GETDATE()

create table #GA
(contact_number int
,Gift_Aid_Amount numeric (9,2))

insert into #GA
select
C3.contact_number
,SUM((FR.Identified_Amount*0.25)+(FR.Indicated_Amount*0.25)+(FR.Pledged_Amount*0.25))
from #CORPAM3300 C3
LEFT JOIN #banked b
on C3.contact_number = b.contact_number
LEFT JOIN cis.gift_aid_declarations gad
ON C3.contact_number = gad.contact_number
LEFT JOIN #FR FR
on C3.contact_number = FR.contact_number
where gad.declaration_number &gt; 0
--AND gad.start_date &gt;= GETDATE()
AND gad.end_date IS NULL
GROUP BY C3.contact_number

create table #MSP
(contact_number int
,MSP numeric (3,2))

insert into #MSP
select
C3.contact_number
,ISNULL (SUM(cc.quantity/100),0) MSP
from #CORPAM3300 C3
LEFT JOIN cis.contact_categories cc
on C3.contact_number = cc.contact_number
where cc.activity = 'MSP'
and cc.valid_to &gt;= GETDATE()
GROUP BY C3.contact_number
--select * from #MSP

select
C3.contact_number
,C3.Name
,#MG.activity_desc
,ISNULL(b.Banked,0) Banked
,SUM(#FR.Target_Amount*#MSP.MSP) Target_x_MSP
,ISNULL(#FR.Target_Amount,0) Target_Amount
,Target_Date = #FR.Target_Month + ' ' + #FR.Target_Year
,ISNULL(#FR.Pledged_Amount,0) Pledged_Amount
,ISNULL(#FR.Indicated_Amount,0) Indicated_Amount
,ISNULL(#FR.Identified_Amount,0) Identified_Amount
,ISNULL(#GA.Gift_Aid_Amount,0) Gift_Aid_Amount
,ISNULL (#MSP.MSP,1) Percentage_Likelihood_of_Giving
,#FR.Project
,Pledged_Adjusted= case 
when Pledged_Amount IS NOT NULL and (#FR.Pledged_Amount - isnull(b.Banked,0))&gt;0 AND #FR.Stage != 'RC'
then (#FR.Pledged_Amount - isnull(b.Banked,0))*#MSP.MSP
else 0
end
,Indicated_Adjusted = case
when (Pledged_Amount = 0 AND #FR.Indicated_Amount IS NOT NULL) AND (#FR.Indicated_Amount - isnull(b.Banked,0))&gt;0 AND #FR.Stage != 'RC'
then (#FR.Indicated_Amount - ISNULL(b.Banked,0))*#MSP.MSP
else 0
end
,Targetted = case
when (Pledged_Amount = 0 AND Indicated_Amount = 0 AND (#FR.Target_Amount - ISNULL(b.Banked,0))&gt;0) AND #FR.Stage != 'RC'
then (#FR.Target_Amount - ISNULL(b.Banked,0))*#MSP.MSP
else 0
end
from #CORPAM3300 C3
LEFT JOIN #MG
ON C3.contact_number = #MG.contact_number
LEFT JOIN #banked b
on C3.contact_number = b.contact_number
LEFT JOIN #FR
ON C3.contact_number = #FR.contact_number
LEFT JOIN #GA
ON C3.contact_number = #GA.contact_number
LEFT JOIN #MSP
on C3.contact_number = #MSP.contact_number
GROUP BY
C3.contact_number
,C3.Name
,#MG.activity_desc
,Banked
,Target_Amount
,target_date
,Pledged_Amount
,Indicated_Amount
,Identified_Amount
,Gift_Aid_Amount
,#FR.Project
,#FR.Target_Month
,#FR.Target_Year
,#MSP.MSP
,#FR.Stage
ORDER BY b.Banked DESC

drop table #CORPAM3300, #FR, #banked, #MG, #GA


