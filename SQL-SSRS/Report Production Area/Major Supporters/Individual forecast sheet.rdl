    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Table_of_Donors">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CORPAM">
            <Value>=Parameters!CORPAM.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--INDIVIDUAL

if OBJECT_ID('tempdb..#CORPAM3300') is not null drop table #CORPAM3300
if OBJECT_ID('tempdb..#FR') is not null drop table #FR
if OBJECT_ID('tempdb..#banked') is not null drop table #banked
if OBJECT_ID('tempdb..#MG') is not null drop table #MG
if OBJECT_ID('tempdb..#CORPVAL') is not null drop table #CORPVAL

create table #CORPAM3300
(contact_number int
,Name varchar (80)
)

INSERT into #CORPAM3300 
select
fr.contact_number
,o.name
from cis.fundraising_requests fr
LEFT JOIN cis.organisations o
ON fr.contact_number = o.organisation_number 
INNER JOIN cis.organisation_categories oc
on o.organisation_number = oc.organisation_number
where (fr.fundraising_request_number IS NOT NULL
	AND DATEPART(yyyy, fr.request_date) = DATEPART (yyyy, getdate()))
	AND (oc.activity = 'CORPAM'
	AND oc.activity_value = @CORPAM
	AND oc.valid_to &gt;= getdate())
UNION
SELECT
	o.organisation_number
	,o.name
FROM cis.organisations o
INNER JOIN wft.wft_financial_history_merge fh
ON o.organisation_number = fh.contact_number
INNER JOIN cis.products p
on fh.product = p.product
inner join cis.organisation_categories oc
on o.organisation_number = oc.organisation_number
WHERE (oc.activity = 'CORPAM'
and oc.activity_value = @CORPAM
and oc.valid_to &gt;= GETDATE())
AND (RIGHT(p.nominal_account,4) = '3200'
and fh.rate = 'CO'
AND DATEPART(yyyy, fh.transaction_date) = '2014')
--datepart (yyyy, getdate())
--select * from #CORPAM3300
--ORDER BY #CORPAM3300.Name

create table #FR
(contact_number int
,Target_Amount numeric (9,2)
,Target_Month varchar(10)
,Target_Year varchar(10)
,target_date datetime
,Pledged_Amount  numeric (9,2)
,Indicated_Amount numeric (9,2)
,Identified_Amount numeric (9,2)
,Project varchar (30))

INSERT INTO #FR
select C3.contact_number
,ISNULL (fr.target_amount,0)
,Expected_Month = datename(MONTH, fr.target_date)
,Expected_Year = DATEname (YYYY, fr.target_date)
,fr.target_date
,Pledged_amount = ISNULL (fr.pledged_amount,0)
,Indicated_Amount = ISNULL (fr.gik_expected_amount,0)
,Identified_Amount = ISNULL (fr.expected_amount,0)
,frt.fundraising_request_type_desc
from #CORPAM3300 C3
LEFT JOIN cis.fundraising_requests fr
on C3.contact_number = fr.contact_number
INNER JOIN cis.fundraising_request_types frt
on fr.fundraising_request_type = frt.fundraising_request_type
WHERE DATEPART (yyyy, fr.target_date) = DATEPART (YYYY, getdate())
ORDER BY C3.contact_number ASC
--select * from #FR

create table #banked
(contact_number int
,Banked numeric (9,2))

insert into #banked
select
C3.contact_number
,SUM(fh.amount)
from #CORPAM3300 C3
INNER JOIN wft.wft_financial_history_merge fh
ON C3.contact_number = fh.contact_number
INNER JOIN cis.products p
on fh.product = p.product
WHERE datepart(YYYY, fh.transaction_date) = datepart (YYYY, getdate())
AND RIGHT(p.nominal_account,4) = '3200'
and fh.rate = 'CO'
GROUP BY C3.contact_number
--select * from #banked

create table #MG
(contact_number int
,activity_desc varchar (40))

insert into #MG
select
C3.contact_number
,oc.activity_value
--,av.activity_value_desc
from #CORPAM3300 C3
LEFT JOIN cis.organisation_categories oc
ON C3.contact_number = oc.organisation_number
--LEFT JOIN cis.activity_values av
--on oc.activity = av.activity
WHERE (oc.activity = 'GMT'
--and oc.activity_value IN ('LD', 'R', 'RD')
AND oc.valid_to &gt;= GETDATE())
--select * from #MG

create table #CORPVAL
(contact_number int
,CORPAM varchar (3))

insert into #CORPVAL
select
C3.contact_number
,oc.activity_value
from #CORPAM3300 C3
left join cis.organisation_categories oc
on C3.contact_number = oc.organisation_number
where oc.activity = 'CORPAM'
and oc.valid_to &gt;= GETDATE()
----select * from #CORPVAL

select
C3.contact_number
,C3.Name
,#CORPVAL.CORPAM
,activity_desc = case
when #MG.activity_desc = 'D' then 'Donor'
when #MG.activity_desc = 'P' then 'Prospect'
when #MG.activity_desc = 'LD' then 'Lapsed_Donor'
else #MG.activity_desc
end
,b.Banked
,#FR.Target_Amount
,Target_Date = #FR.Target_Month + ' ' + #FR.Target_Year
,#FR.Pledged_Amount
,#FR.Indicated_Amount
,#FR.Identified_Amount
,#FR.Project
,Pledged_Adjusted= case 
when #FR.Pledged_Amount IS NOT NULL and (#FR.Pledged_Amount - isnull(b.Banked,0))&gt;0
then (#FR.Pledged_Amount - isnull(b.Banked,0))
else 0
end
,Indicated_Adjusted = case
when (Pledged_Amount = 0 AND #FR.Indicated_Amount IS NOT NULL) AND (#FR.Indicated_Amount - isnull(b.Banked,0))&gt;0
then ((#FR.Indicated_Amount - ISNULL(b.Banked,0))*0.714)
else 0
end
,Targetted = case
when (Pledged_Amount = 0 AND Indicated_Amount = 0 AND #MG.activity_desc = 'D' AND (#FR.Target_Amount - ISNULL(b.Banked,0))&gt;0) 
then ((#FR.Target_Amount - ISNULL(b.Banked,0))*0.33)
when (Pledged_Amount = 0 AND Indicated_Amount = 0 AND (#MG.activity_desc = 'P' OR #MG.activity_desc = 'LD') AND (#FR.Target_Amount - ISNULL(b.Banked,0))&gt;0) 
then ((#FR.Target_Amount - ISNULL(b.Banked,0))*0.05)
else 0
end
,case when #CORPVAL.CORPAM = 'KL' then 160000
when #CORPVAL.CORPAM = 'BB' then 160000
when #CORPVAL.CORPAM = 'JG' then 160000
end Target
from #CORPAM3300 C3
LEFT JOIN #MG
ON C3.contact_number = #MG.contact_number
LEFT JOIN #banked b
on C3.contact_number = b.contact_number
LEFT JOIN #FR
ON C3.contact_number = #FR.contact_number
LEFT JOIN #CORPVAL
on C3.contact_number = #CORPVAL.contact_number
ORDER BY C3.contact_number ASC
--#FR.target_date ASC

drop table #CORPAM3300, #FR, #banked, #MG;</CommandText>
