        <CommandText>if object_id('tempdb..#CORPAM_Actions') is not null drop table #CORPAM_Actions
if object_id('tempdb..#Related_Actions') is not null drop table #Related_Actions
if object_id('tempdb..#mergedactiontable') is not null drop table #mergedactiontable

--create table listing actions where account managers are associated as actioners
CREATE table #CORPAM_Actions
(action_number int
,actioner_number int
,actioner_name varchar(80))

insert into #CORPAM_Actions
select ca.action_number
,ca.contact_number
,c.label_name
from cis.contact_actions ca
inner join cis.contacts c
ON ca.contact_number = c.contact_number
INNER JOIN cis.actions a
ON a.action_number = ca.action_number
where (ca.contact_number IN ('1571751', '1799340', '1700023', '24249', '1827933', '1890379', '1495298', '1858988', '1676730', '32830', '1871866') 
AND ca.type = 'A')
--AND a.action_priority IN ('TMDN', 'TMDF', 'TMDG')
AND a.completed_on IS NULL
--select * from #CORPAM_Actions
--order by #CORPAM_Actions.action_number

--create table listing related to contacts for CORPAM actioners table
CREATE table #Related_Actions
(action_number int
,related_number int
,related_name varchar(80))

insert into #Related_Actions
select #CORPAM_Actions.action_number
,ca.contact_number
,c.label_name
from cis.contact_actions ca
LEFT JOIN #CORPAM_Actions
ON #CORPAM_Actions.action_number = ca.action_number
inner join cis.contacts c
ON ca.contact_number = c.contact_number
INNER JOIN cis.actions a
ON ca.action_number = a.action_number
where ca.type = 'R'
--AND a.action_priority IN ('TMDN', 'TMDF', 'TMDG')
AND a.completed_on IS NULL;
--select * from #Related_Actions
--INNER JOIN #CORPAM_Actions
--ON #Related_Actions.action_number = #CORPAM_Actions.action_number
--ORDER BY #Related_Actions.related_number

--merge temp tables to create one containing actioner and related to contact and drop original temp tables
create table #mergedactiontable
(action_number int
,related_to_contact_number int
,related_to_name varchar(80)
,actioner_number int
,actioner_name varchar(80))

insert into #mergedactiontable
select #CORPAM_Actions.action_number
,#Related_Actions.related_number
,#Related_Actions.related_name
,#CORPAM_Actions.actioner_number
,#CORPAM_Actions.actioner_name
FROM #CORPAM_Actions
LEFT JOIN #Related_Actions
ON #CORPAM_Actions.action_number = #Related_Actions.action_number
drop table #CORPAM_Actions, #Related_Actions;
--select * from #mergedactiontable
--ORDER BY #mergedactiontable.action_number

--CTE pull actions from merged table according to priority 
with nextaction
as
(select
ma.action_number nextnumber
,ma.related_to_contact_number nextconnumber
,a.action_desc nextdesc
,a.action_text nexttext
,a.deadline nextdeadline
,ap.action_priority_desc
,ROW_NUMBER() OVER (PARTITION BY ma.related_to_contact_number ORDER BY a.deadline ASC) AS [rn]
from #mergedactiontable ma
LEFT JOIN cis.actions a
ON ma.action_number = a.action_number
INNER JOIN cis.action_priorities ap
on a.action_priority = ap.action_priority
WHERE a.action_priority = 'TMDN'
AND a.completed_on IS NULL
AND ((DATEPART(yyyy,a.deadline) = DATEPART(yyyy,getdate())
AND (DATEPART(mm,a.deadline) &lt;= DATEPART(mm,getdate()+4)))
OR (a.deadline &lt; getdate()))),
--))
--(DATEPART(mm,a.deadline) = DATEPART(mm,getdate()) OR ))
--select * from nextaction
--order by nextaction.nextnumber;
followup
as
(select
ma.action_number follownumber
,ma.related_to_contact_number followconnumber
,a.action_desc followdesc
,a.action_text followtext
,a.deadline followdeadline
,ap.action_priority_desc
,rn = ROW_NUMBER() OVER (PARTITION BY ma.related_to_contact_number ORDER BY a.deadline ASC)
from #mergedactiontable ma
LEFT JOIN cis.actions a
ON ma.action_number = a.action_number
INNER JOIN cis.action_priorities ap
on a.action_priority = ap.action_priority
WHERE a.action_priority = 'TMDF'
AND a.completed_on IS NULL),
--select * from followup
--order by followup.follownumber;
goal
as
(select
ma.action_number goalnumber
,ma.related_to_contact_number goalconnumber
,a.action_desc goaldesc
,a.action_text goaltext
,a.deadline goaldeadline
,ap.action_priority_desc
,ROW_NUMBER() OVER (PARTITION BY ma.related_to_contact_number ORDER BY a.deadline ASC) AS [rn]
from #mergedactiontable ma
LEFT JOIN cis.actions a
ON ma.action_number = a.action_number
INNER JOIN cis.action_priorities ap
on a.action_priority = ap.action_priority
WHERE a.action_priority = 'TMDG'
AND a.completed_on IS NULL),
nextfinal as
(select
nextaction.nextconnumber
,nextaction.nextnumber
,nextaction.action_priority_desc
,nextaction.nextdesc
,nextaction.nexttext
,nextaction.nextdeadline
from nextaction
where nextaction.rn = 1),
--select * from nextfinal
--order by nextfinal.nextnumber
followfinal as
(select
followup.followconnumber
,followup.follownumber
,followdesc
,followdeadline
from followup
where followup.rn = 1),
--select * from followfinal
--order by followfinal.follownumber
goalfinal as
(select
goal.goalconnumber
,goal.goalnumber
,goal.goaldesc
,goal.goaldeadline
from goal
where goal.rn = 1),
emails as
(select
ma.related_to_contact_number
,comm.number
,ROW_NUMBER() OVER(partition by comm.contact_number order by comm.amended_on DESC) rn
from #mergedactiontable ma
left join cis.communications comm
on ma.related_to_contact_number = comm.contact_number
where comm.device IN ('EC','EM','WR')),
email1 as
(select * from emails
where emails.rn = 1),
phone as
(select
ma.related_to_contact_number
,comm.std_code
,comm.number
,case
when comm.device = 'MB' then 'Mobile'
when comm.device = 'S' then 'Switchboard'
when comm.device = 'T' then 'Home_Phone'
when comm.device = 'TD' then 'Direct_Phone'
when comm.device ='TW' then 'Work_Phone'
else comm.device
end type
,ROW_NUMBER() OVER(partition by comm.contact_number order by comm.amended_on DESC) rn
from #mergedactiontable ma
left join cis.communications comm
on ma.related_to_contact_number = comm.contact_number
where comm.device IN ('MB','S','T','TD','TW')),
phone1 as
(select * from phone
where phone.rn = 1)

--select * from goalfinal
--ORDER bY goalfinal.goalnumber
select distinct
#mergedactiontable.related_to_contact_number contact_number
,#mergedactiontable.related_to_name Name
--,#mergedactiontable.actioner_number
,Actioner = case #mergedactiontable.actioner_name 
when 'Miss E M Hoogstraten' then 'Emily'
when 'Mr B Eydmann' then 'Ben'
when 'Mr M P Patten' then 'Matt'
else #mergedactiontable.actioner_name
end 
--,nextfinal.nextnumber
,nextfinal.nextdesc Action_Description
,nextfinal.nexttext Action_Text
,Deadline = CONVERT(VARCHAR(10), nextfinal.nextdeadline, 3)
--,followfinal.follownumber
,followfinal.followdesc
,followdeadline = CONVERT (VARCHAR(10),followfinal.followdeadline, 3)
--,goalfinal.goalnumber
,goalfinal.goaldesc
--,goaldeadline = CONVERT (VARCHAR(10), goalfinal.goaldeadline, 3)
,ISNULL(phone1.std_code,' ') + ' ' + ISNULL(phone1.number,' ') Phone_Number
,phone1.type Phone_Type
,email1.number Email
,a.address
from #mergedactiontable
LEFT JOIN nextfinal
ON #mergedactiontable.related_to_contact_number = nextfinal.nextconnumber
LEFT JOIN followfinal
ON #mergedactiontable.related_to_contact_number = followfinal.followconnumber
LEFT JOIN goalfinal
ON #mergedactiontable.related_to_contact_number = goalfinal.goalconnumber
LEFT JOIN cis.contacts c
on #mergedactiontable.related_to_contact_number = c.contact_number
INNER JOIN cis.addresses a
on c.address_number = a.address_number
LEFT JOIN email1
on #mergedactiontable.related_to_contact_number = email1.related_to_contact_number
LEFT JOIN phone1
on #mergedactiontable.related_to_contact_number = phone1.related_to_contact_number
order by #mergedactiontable.related_to_contact_number

drop table #mergedactiontable</CommandText>
