<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Lists applications sent to trusts with fundraising requests sent in the last calendar month
Data Source: DiabetesDWH</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>/Data Sources/SharedDataSource_DiabetesDWH</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>08e08115-3466-41bc-a8ba-d38989b92578</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>---------------------------------------------------------------------------------------------------
----------------------Start LJS Version--------------------------------------------------------
---------------------------------------------------------------------------------------------------

DECLARE	@StartPreviousMonth	DATETIME = DATEADD(m, -1, DATEADD(m, DATEDIFF(m, 0, GETDATE()), 0))
DECLARE	@EndPreviousMonth	DATETIME = DATEADD(d, -1, DATEADD(m, DATEDIFF(m, 0, GETDATE()), 0))

;
with CORPAM as
(select
o.organisation_number
,o.name
,oc.activity_value
,fr.fundraising_request_number
from cis.fundraising_requests fr
inner join cis.organisations o
on fr.contact_number = o.organisation_number
Inner join cis.organisation_categories oc
on fr.contact_number = oc.organisation_number
where (oc.activity = 'CORPAM'
		AND oc.activity_value IN ('KL','BB','OJ','LSM','AW','AHA') --GG 19-11-2015
		AND oc.valid_to &gt;= @StartPreviousMonth) -- changed from GETDATE() GG 10-07-2015
AND fr.fundraising_request_number IS NOT NULL),
--AND fr.request_date &gt;= dateadd(d,-365,getdate())
--AND datepart(yyyy,fr.target_date) = DATEPART(YYYY,GETDATE()))

report as (select
CORPAM.organisation_number
,CORPAM.name
,case
when CORPAM.activity_value = 'OJ' then 'Odhran'
when CORPAM.activity_value = 'BB' then 'Bhavini'
when CORPAM.activity_value = 'KL' then 'Kate'
when CORPAM.activity_value = 'AHA' then 'Amelia' --GG 19-11-2015
when CORPAM.activity_value = 'AW' then 'Alison' --GG 16-07-2015
end CORPAM
,st.sub_topic_desc
--,convert (varchar(10),cl.dated,103) Application_Sent
,cast(cl.dated as date) as Application_Sent
,communications_log.subject
,fr.request_description
,frs.fundraising_request_stage_desc
,frt.fundraising_request_type_desc
,fs.fundraising_status_desc
,fr.target_amount
--,convert(varchar(10),fr.target_date,103) Target_Date
,cast(fr.target_date as date) as Target_Date
from CORPAM
left JOIN custom.cis_comms_log_detail cl
on CORPAM.organisation_number = cl.contact_number
left join cis.communications_log
on cl.communications_log_number = communications_log.communications_log_number
left join cis.topics t
on cl.topic = t.topic
left join cis.sub_topics st
on cl.topic = st.topic and cl.sub_topic = st.sub_topic
left join cis.fundraising_requests fr
on CORPAM.fundraising_request_number = fr.fundraising_request_number
LEFT JOIN cis.fundraising_request_stages frs
on fr.fundraising_request_stage = frs.fundraising_request_stage
LEFT JOIN cis.fundraising_request_types frt
on fr.fundraising_request_type = frt.fundraising_request_type
LEFT JOIN cis.fundraising_statuses fs
on fr.fundraising_status = fs.fundraising_status
WHERE (cl.topic = 'CORP16'
and cl.sub_topic = 'T004')
/*AND (datepart(mm,cl.dated) = DATEPART(mm,getdate())-1
AND datepart(yyyy,cl.dated) = DATEPART(yyyy,getdate()))
AND */


/* in the previous calendar month*/
AND cl.dated between @StartPreviousMonth and @EndPreviousMonth)	

/*in the last Month*/
--cl.dated between dateadd(mm,-1,getdate()) and getdate()

select * from report
where report.fundraising_request_stage_desc != 'Request Closed'
ORDER BY Application_Sent ASC

---------------------------------------------------------------------------------------------------
----------------------End LJS Version---------------------------------------------------------
---------------------------------------------------------------------------------------------------



