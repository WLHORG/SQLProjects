    <DataSet Name="RepParCampaigns">
      <Query>
        <DataSourceName>DiabetesDWH</DataSourceName>
        <CommandText>SELECT DISTINCT Campaign, Campaign + ' - ' + campaign_desc AS campaign_and_description
FROM         wft.wft_source_code_lookup
WHERE     (Campaign IS NOT NULL) AND (Campaign LIKE '_05%' OR
                      Campaign LIKE '__[AD]D[MR]%')
ORDER BY Campaign DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Campaign">
          <DataField>Campaign</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
