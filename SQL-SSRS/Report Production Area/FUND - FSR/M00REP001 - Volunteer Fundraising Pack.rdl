    <DataSet Name="DUK_Region">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT('UNKNOWN')  AS DUK_region FROM  [cis].[duk_fundraising_areas] 
UNION ALL 
SELECT DISTINCT(LTRIM(RTRIM(fund_region_desc))) as DUK_region
FROM  [cis].[duk_fundraising_areas] fa 
INNER JOIN [cis].[duk_fund_region_hierarchy] fh ON fa.rf_area = fh.rf_area
INNER JOIN [cis].[duk_fundraising_regions] fr ON fh.fund_region = fr.fund_region
INNER JOIN [custom].[cis_contact_to_nation] ctn ON ctn.branch =  fh.branch
ORDER BY 1 ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="DUK_region">
          <DataField>DUK_region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
