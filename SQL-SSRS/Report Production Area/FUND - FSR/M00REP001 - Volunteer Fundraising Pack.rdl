if object_id ('tempdb..#VTFV') is not null drop table #VTFV
select distinct cis.contact_number 
into #VTFV
from openquery(cislive, 'select distinct contact_number 
							from contact_categories 
							where activity = ''VT''
							and activity_value = ''FPV''
							and sysdate &gt; valid_to') as cis;

if object_id('tempdb..#vfp') is not null drop table #vfp
create table #vfp 
(segment varchar(25)
 ,[source] varchar(10)
,source_desc varchar(60) 
,contact_number int
,label_name varchar(80)
,under_18 char(1)
,town varchar(35)
,county varchar(35)
,duk_region varchar(80)
,dated date
,dated_weekday varchar(10)
,contact_date date
,contact_type varchar(30)
,all_sub_topics varchar(4000)
,IF_activity char(1)
,CFR_or_Region_Topic char(1));


if object_id('tempdb..#Volunteer_Fundraising_Pack') is not null drop table #Volunteer_Fundraising_Pack
(select cis.communications_log_number,
cis.[source],
cis.source_desc,
cis.contact_number,
cis.label_name,
case when floor(datediff(dd,cis.date_of_birth,getdate())/365.23076923074) &lt;18 then 'Y' else 'N' end as Under_18,
vu.town,
vu.county,
isnull(replace(replace(replace(ctn.duk_region,'Diabetes U K (',''),' region)',''),')',''),'UNKNOWN') as DUK_region,
cis.topic,	
cis.sub_topic,	
cast(cis.dated as date) as dated,
cis.document_type
into #Volunteer_Fundraising_Pack
from OPENQUERY(CISLIVE, 'SELECT cl.communications_log_number,
										cl.source,
										s.source_desc,
										cl.contact_number,
										c.label_name,
										c.date_of_birth,
										cls.topic,	
										cls.sub_topic,	
										cl.dated,
										cl.document_type
							FROM communications_log cl
							INNER JOIN communications_log_subjects cls ON cl.communications_log_number = cls.communications_log_number
							INNER JOIN contacts c ON cl.contact_number = c.contact_number
							LEFT JOIN sources s ON cl.source=s.source
							WHERE cls.topic = ''VFP''') as cis





--left join custom.cis_contact_to_nation as ctn on cis.contact_number = ctn.contact_number --  REPLACED WITH NEW FUNDRAISING REGION BELOW 
left join  (SELECT contact_number, fund_region_desc as duk_region  FROM  [cis].[duk_fundraising_areas] fa 
INNER JOIN [cis].[duk_fund_region_hierarchy] fh ON fa.rf_area = fh.rf_area
INNER JOIN [cis].[duk_fundraising_regions] fr ON fh.fund_region = fr.fund_region
INNER JOIN [custom].[cis_contact_to_nation] ctn ON ctn.branch =  fh.branch) as ctn on cis.contact_number = ctn.contact_number



left join custom.cis_contact_selection_header as vu on cis.contact_number = vu.contact_number)


if object_id('tempdb..#cc') is not null drop table #cc
select distinct cc.contact_number 
into #cc
from cis.contact_categories as cc
inner join #Volunteer_Fundraising_Pack as vfp2 on cc.contact_number=vfp2.contact_number
where cc.activity like 'IF%' 
and getdate() between cc.valid_from and cc.valid_to
and cc.valid_from between vfp2.dated and dateadd(dd,14,vfp2.dated) 


if object_id('tempdb..#cfr') is not null drop table #cfr
select distinct cfr.contact_number
into #cfr
from [custom].[cis_contact_topic_subtopic] as cfr
inner join #Volunteer_Fundraising_Pack as vfp2 on cfr.contact_number=vfp2.contact_number
where (cfr.topic like 'CFR%'
		or cfr.topic like 'EA%'
		or cfr.topic like 'EM%'
		or cfr.topic like 'ER%'
		or cfr.topic like 'LR%'
		or cfr.topic like 'NI%'
		or cfr.topic like 'NW%'
		or cfr.topic like 'NY%'
		or cfr.topic like 'SC%'
		or cfr.topic like 'SE%'
		or cfr.topic like 'SW%'
		or cfr.topic like 'WA%'
		or cfr.topic like 'WM%')
and cfr.dated between vfp2.dated and dateadd(dd,14,vfp2.dated)

--001 Download and Post 
insert into #vfp
(segment
,[source]
,source_desc
,contact_number
,label_name
,under_18
,town
,county
,duk_region
,dated
,dated_weekday
,contact_date
,contact_type
,all_sub_topics
,IF_activity
,CFR_or_Region_Topic)
select distinct '001 Download &amp; Post' as segment
,vfp.source
,vfp.source_desc
,vfp.contact_number
,vfp.label_name
,vfp.Under_18
,vfp.town
,vfp.county
,vfp.DUK_region
,vfp.dated
,datename(dw,vfp.dated) as dated_weekday
,(select top 1 vfp1.dated
	from #Volunteer_Fundraising_Pack as vfp1
	where vfp.contact_number=vfp1.contact_number
	and vfp1.sub_topic in ('CALL','NOAN')
	order by dated desc)

,(select top 1 st.sub_topic_desc
	from #Volunteer_Fundraising_Pack as vfp1
	inner join cis.sub_topics as st on vfp1.topic=st.topic
								and vfp1.sub_topic=st.sub_topic
	where vfp.contact_number=vfp1.contact_number
	and vfp1.sub_topic in ('CALL','NOAN')
	order by dated desc)

, stuff(
		(select ', ' + st.sub_topic_desc
			from #Volunteer_Fundraising_Pack as vfp1
			inner join cis.sub_topics as st on vfp1.topic=st.topic
								and vfp1.sub_topic=st.sub_topic
			where vfp.contact_number=vfp1.contact_number
			order by vfp1.dated desc
          for xml path ('')
							), 1, 1, '')  as all_sub_topics

, case when cc1.contact_number is not null then 1 else 0 end
, case when cfr1.contact_number is not null then 1 else 0 end							 
from #Volunteer_Fundraising_Pack as vfp
inner join #Volunteer_Fundraising_Pack as vfp1 on vfp.contact_number=vfp1.contact_number
left join #cc as cc1 on vfp.contact_number=cc1.contact_number 
left join #cfr as cfr1 on vfp.contact_number=cfr1.contact_number
where vfp.topic = 'VFP'
and vfp.sub_topic = 'DOWN'
and vfp.dated between dateadd(dd,-10,getdate()) and getdate()
and vfp1.topic = 'VFP'
and vfp1.sub_topic = 'POST' 
and vfp1.dated between dateadd(dd,-10,getdate()) and getdate()
--and (vfp.[source] in (@source_code)
--		--or vfp1.[source] in (@source_code)
--		)
and vfp.duk_region in (@duk_region)
and vfp.contact_number not in (select vfp1.contact_number
								from #Volunteer_Fundraising_Pack as vfp1
								where vfp1.topic = 'VFP' 
								and vfp1.sub_topic in ('FOLW'
														,'EMAI'
														,'NFOL'
														,'NOAN' --added 27102014 LJS  
														,'GLC'--added 27102014 LJS 
														,'FOLW'--added 27102014 LJS 
														,'NFOL'--added 27102014 LJS 
														,'PC'--added 27102014 LJS 
														,'TYC'))--added 27102014 LJS 					
and vfp.contact_number not in (select contact_number
								from #VTFV)


