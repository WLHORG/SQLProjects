<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>SharedDataSource_DiabetesDWH</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b71ca1d3-9757-493f-8f5b-24e49451d08e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="MainSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ed">
            <Value>=Parameters!ed.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  p.company,
		r.nominal_account_suffix reg_anal_code,
		i.contact_number,
		c.label_name,
		i.invoice_number,
		right(p.nominal_account,4) activity_code,
		i.invoice_date,
		fh.transaction_date,
		i.invoice_pay_status,
		datediff(dy,i.invoice_date,@ed) days_os,
		SUM(bta.amount)-max(i.amount_paid) balance
from cis.invoices i
inner join cis.contacts c on i.contact_number=c.contact_number
 inner join cis.batch_transaction_analysis bta
					on i.batch_number=bta.batch_number
					and i.transaction_number=bta.transaction_number
					and i.invoice_pay_status in ('N')
					and i.invoice_date&lt;=@ed
				inner join cis.products p
					on p.product=bta.product
				inner join cis.rates r
					on r.rate=bta.rate
					and r.product=bta.product
					and not(bta.amount is null)
				left join cis.invoice_payment_history iph
					on i.invoice_number=iph.invoice_number
				left join cis.financial_history fh
					on fh.batch_number=iph.batch_number
					and fh.transaction_number=iph.transaction_number
where (fh.transaction_date is null or fh.transaction_date&gt;=@ed)
group by p.company,
			r.nominal_account_suffix,
			i.contact_number,
			c.label_name,
			i.invoice_number,
			right(p.nominal_account,4),
			i.invoice_date,
			fh.transaction_date,
			i.invoice_pay_status
having (SUM(bta.amount)-max(amount_paid))&gt;0
order by i.invoice_number</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
