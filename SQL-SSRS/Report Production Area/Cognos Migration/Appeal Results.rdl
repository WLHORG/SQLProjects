  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":Appeal">
            <Value>=Parameters!Appeal.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":transactiondate">
            <Value>=Parameters!transactiondate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT "CashResults"."CAMPAIGN" "CAMPAIGN",
       "CashResults"."APPEAL" "APPEAL",
       "CashResults"."SEGMENT" "SEGMENT",
       "CashResults"."GIFTS" "GIFTS",
       round(sum("CashResults"."ROI"),2) "ROI",
       "DdResults"."DDs" "DDs",
       "CashResults"."SOURCE_DESC" "SOURCE_DESC",
       sum("CashResults"."ACTUAL_COUNT") "ACTUAL_COUNT",
       sum("CashResults"."totalCost") "totalCost",
       round(sum("CashResults"."UnitCost"),2) "UnitCost",
       round(CASE
           WHEN "CashResults"."ACTUAL_COUNT"&gt;0 THEN "CashResults"."GIFTS"/nullif("CashResults"."ACTUAL_COUNT", 0)
           ELSE NULL
       END,4) "CashRespRate",
       "CashResults"."Income" "Income",
       round(CASE
           WHEN "CashResults"."GIFTS"&gt;0 THEN "CashResults"."Income"/nullif("CashResults"."GIFTS", 0)
           ELSE NULL
       END,4) "AvgGift",
       sum("CashResults"."Profit") "Profit",
       round(sum("CashResults"."IncomePerMail"),2) "IncomePerMail",
       sum("DdResults"."AnnualValue") "AnnualValue",
       round(CASE
           WHEN sum("CashResults"."ACTUAL_COUNT")&gt;0 THEN min("DdResults"."DDs")/nullif(sum("CashResults"."ACTUAL_COUNT"), 0)
           ELSE NULL
       END,4) "DdResponseRate",
       round(CASE
           WHEN min("DdResults"."DDs")&gt;0 THEN sum("DdResults"."AnnualValue")/nullif(min("DdResults"."DDs"), 0)
       END,4) "AvgAnnualValue"
FROM
  (SELECT "Query2"."CAMPAIGN" "CAMPAIGN",
          "Query2"."APPEAL" "APPEAL",
          "Query2"."SEGMENT" "SEGMENT",
          sum("Query2"."ACTUAL_COUNT") "ACTUAL_COUNT",
          sum("Query2"."COST") "COST",
          sum("Query2"."FIXED_COST") "FIXED_COST",
          "Query2"."source" "source",
          "Query2"."AMOUNT" "Income",
          "fromSources"."SOURCE_DESC" "SOURCE_DESC",
          sum("Query2"."AMOUNT"/nullif("Query2"."FIXED_COST", 0)) "ROI",
          sum("Query2"."AMOUNT" - "Query2"."FIXED_COST") "Profit",
          CASE
              WHEN (coalesce(sum("Query2"."ACTUAL_COUNT"), 0))&gt;0 THEN min("Query2"."AMOUNT")/nullif((coalesce(sum("Query2"."ACTUAL_COUNT"), 0)), 0)
              ELSE NULL
          END "IncomePerMail",
          sum((coalesce("Query2"."FIXED_COST", 0))+(coalesce("Query2"."COST", 0))*(coalesce("Query2"."ACTUAL_COUNT", 0))) "totalCost",
          sum(((coalesce("Query2"."FIXED_COST", 0))+(coalesce("Query2"."COST", 0))*(coalesce("Query2"."ACTUAL_COUNT", 0))))/nullif(sum("Query2"."ACTUAL_COUNT"), 0) "UnitCost",
          "Query2"."GIFTS" "GIFTS"
   FROM
     (SELECT "FromSegments"."CAMPAIGN" "CAMPAIGN",
             "FromSegments"."APPEAL" "APPEAL",
             "FromSegments"."SEGMENT" "SEGMENT",
             "FromSegments"."SEGMENT_DESC" "SEGMENT_DESC",
             sum("FromSegments"."ACTUAL_COUNT") "ACTUAL_COUNT",
             sum("FromSegments"."COST") "COST",
             sum("FromSegments"."FIXED_COST") "FIXED_COST",
             "FromSegments"."source" "source",
             "Query1"."AMOUNT" "AMOUNT",
             "Query1"."GIFTS" "GIFTS"
      FROM
        (SELECT "SEGMENTS"."CAMPAIGN" "CAMPAIGN",
                "SEGMENTS"."APPEAL" "APPEAL",
                "SEGMENTS"."SEGMENT" "SEGMENT",
                "SEGMENTS"."SEGMENT_DESC" "SEGMENT_DESC",
                sum("SEGMENTS"."ACTUAL_COUNT") "ACTUAL_COUNT",
                sum("SEGMENTS"."COST") "COST",
                sum("SEGMENTS"."FIXED_COST") "FIXED_COST",
                "SEGMENTS"."CAMPAIGN"||"SEGMENTS"."APPEAL"||"SEGMENTS"."SEGMENT" "source"
         FROM "SEGMENTS" "SEGMENTS"
         WHERE SUBSTR("SEGMENTS"."CAMPAIGN"||"SEGMENTS"."APPEAL"||"SEGMENTS"."SEGMENT",1 ,6)=:Appeal
         GROUP BY "SEGMENTS"."CAMPAIGN",
                  "SEGMENTS"."APPEAL",
                  "SEGMENTS"."SEGMENT",
                  "SEGMENTS"."SEGMENT_DESC",
                  "SEGMENTS"."CAMPAIGN"||"SEGMENTS"."APPEAL"||"SEGMENTS"."SEGMENT") "FromSegments"
      LEFT OUTER JOIN
        (SELECT sum("FromFHD"."AMOUNT") "AMOUNT",
                "FromFHD"."SOURCE" "SOURCE",
                count("FromFHD"."SOURCE") "GIFTS"
         FROM
           (SELECT DISTINCT "FINANCIAL_HISTORY_DETAILS"."AMOUNT" "AMOUNT",
                            "FINANCIAL_HISTORY_DETAILS"."SOURCE" "SOURCE",
                            "FINANCIAL_HISTORY_DETAILS"."BATCH_NUMBER" "BATCH_NUMBER",
                            "FINANCIAL_HISTORY_DETAILS"."TRANSACTION_NUMBER" "TRANSACTION_NUMBER"
            FROM "FINANCIAL_HISTORY_DETAILS" "FINANCIAL_HISTORY_DETAILS"
            WHERE "FINANCIAL_HISTORY_DETAILS"."PRODUCT" NOT LIKE 'SRRG%'
              AND SUBSTR("FINANCIAL_HISTORY_DETAILS"."SOURCE",1 ,6)=:Appeal) "FromFHD",
           (SELECT DISTINCT "FINANCIAL_HISTORY"."BATCH_NUMBER" "BATCH_NUMBER",
                            "FINANCIAL_HISTORY"."TRANSACTION_NUMBER" "TRANSACTION_NUMBER",
                            "FINANCIAL_HISTORY"."TRANSACTION_DATE" "TRANSACTION_DATE"
            FROM "FINANCIAL_HISTORY" "FINANCIAL_HISTORY"
            WHERE "FINANCIAL_HISTORY"."TRANSACTION_DATE"&lt;=:transactiondate) "FromFH"
         WHERE "FromFHD"."BATCH_NUMBER"="FromFH"."BATCH_NUMBER"
           AND "FromFHD"."TRANSACTION_NUMBER"="FromFH"."TRANSACTION_NUMBER"
         GROUP BY "FromFHD"."SOURCE") "Query1" ON "FromSegments"."source"="Query1"."SOURCE"
      GROUP BY "FromSegments"."CAMPAIGN",
               "FromSegments"."APPEAL",
               "FromSegments"."SEGMENT",
               "FromSegments"."SEGMENT_DESC",
               "FromSegments"."source",
               "Query1"."AMOUNT",
               "Query1"."GIFTS") "Query2",
                                 "SOURCES" "fromSources"
   WHERE "Query2"."source"="fromSources"."SOURCE"
   GROUP BY "Query2"."CAMPAIGN",
            "Query2"."APPEAL",
            "Query2"."SEGMENT",
            "Query2"."source",
            "Query2"."AMOUNT",
            "fromSources"."SOURCE_DESC",
            "Query2"."GIFTS") "CashResults"
LEFT OUTER JOIN
  (SELECT "Query3"."SOURCE" "SOURCE",
          sum("Query3"."AnnualValue") "AnnualValue",
          count(DISTINCT "Query3"."ORDER_NUMBER") "DDs"
   FROM
     (SELECT "ORDERS"."ORDER_NUMBER" "ORDER_NUMBER",
             "ORDERS"."ORDER_TYPE" "ORDER_TYPE",
             "ORDERS"."CANCELLATION_REASON" "CANCELLATION_REASON",
             "ORDER_DETAILS"."SOURCE" "SOURCE",
             sum("ORDERS"."FREQUENCY_AMOUNT") "FREQUENCY_AMOUNT",
             "ORDERS"."PAYMENT_FREQUENCY" "PAYMENT_FREQUENCY",
             CASE WHEN min("ORDERS"."PAYMENT_FREQUENCY") IN ('I',
                                                             'M') THEN 12*sum("ORDERS"."FREQUENCY_AMOUNT") WHEN min("ORDERS"."PAYMENT_FREQUENCY") IN ('Q',
                                                                                                                                                      'J') THEN 4*sum("ORDERS"."FREQUENCY_AMOUNT") WHEN min("ORDERS"."PAYMENT_FREQUENCY")='A' THEN sum("ORDERS"."FREQUENCY_AMOUNT") END "AnnualValue",
"ORDERS"."ORDER_DATE" "ORDER_DATE"
      FROM "ORDERS" "ORDERS",
           "ORDER_DETAILS" "ORDER_DETAILS"
WHERE ("ORDERS"."CANCELLATION_REASON" IS NULL
       OR "ORDERS"."CANCELLATION_REASON"&lt;&gt;'ERR')
AND "ORDER_DETAILS"."SOURCE" LIKE 'J05001%'
AND "ORDERS"."ORDER_NUMBER"="ORDER_DETAILS"."ORDER_NUMBER"
      GROUP BY "ORDERS"."ORDER_NUMBER",
               "ORDERS"."ORDER_TYPE",
               "ORDERS"."CANCELLATION_REASON",
               "ORDER_DETAILS"."SOURCE",
               "ORDERS"."PAYMENT_FREQUENCY",
               "ORDERS"."ORDER_DATE") "Query3",
(SELECT DISTINCT "ORDER_DETAILS"."ORDER_NUMBER" "ORDER_NUMBER",
                 "ORDER_DETAILS"."PRODUCT" "PRODUCT"
 FROM "ORDER_DETAILS" "ORDER_DETAILS"
 WHERE "ORDER_DETAILS"."PRODUCT" LIKE 'SRRG%') "Query4"
WHERE SUBSTR("Query3"."SOURCE",1 ,6)=:Appeal
AND "Query3"."ORDER_NUMBER"="Query4"."ORDER_NUMBER"
   GROUP BY "Query3"."SOURCE") "DdResults" ON "CashResults"."source"="DdResults"."SOURCE"
GROUP BY "CashResults"."CAMPAIGN",
     "CashResults"."APPEAL",
     "CashResults"."SEGMENT",
     "CashResults"."Income",
     "CashResults"."SOURCE_DESC",
     "DdResults"."DDs",
     "CashResults"."GIFTS",
     CASE
     WHEN "CashResults"."GIFTS"&gt;0 THEN "CashResults"."Income"/nullif("CashResults"."GIFTS", 0)
     ELSE NULL
 END ,
     CASE
     WHEN "CashResults"."ACTUAL_COUNT"&gt;0 THEN "CashResults"."GIFTS"/nullif("CashResults"."ACTUAL_COUNT", 0)
     ELSE NULL
 END</CommandText>
      </Query>
      <Fields>
        <Field Name="CAMPAIGN">
          <DataField>CAMPAIGN</DataField>
