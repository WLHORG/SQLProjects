          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT "Query3"."SOURCE" "SOURCE",
       "Sources"."SOURCE_DESC" "SOURCE_DESC",
       sum("Query3"."annualamount") "annualamount",
       sum("Query3"."Regular_Gifts") "Regular_Gifts"
FROM
  (SELECT "Query2"."SOURCE" "SOURCE",
          count("Query2"."SOURCE") "Regular_Gifts",
          sum("Query2"."annualamount") "annualamount"
   FROM
     (SELECT "ORDERS"."ORDER_NUMBER" "ORDER_NUMBER",
             "ORDERS"."ORDER_TYPE" "ORDER_TYPE",
             "ORDERS"."PAYMENT_METHOD" "PAYMENT_METHOD",
             "ORDERS"."PAYMENT_FREQUENCY" "PAYMENT_FREQUENCY",
             sum("ORDERS"."FREQUENCY_AMOUNT") "FREQUENCY_AMOUNT",
             "ORDERS"."SOURCE" "SOURCE",
             CASE WHEN (min("ORDERS"."PAYMENT_FREQUENCY") IN ('I',
                                                              'M')) THEN 12 WHEN (min("ORDERS"."PAYMENT_FREQUENCY") IN ('J',
                                                                                                                        'Q')) THEN 4 WHEN (min("ORDERS"."PAYMENT_FREQUENCY")='A') THEN 1 END *sum("ORDERS"."FREQUENCY_AMOUNT") "annualamount"
      FROM "ORDERS" "ORDERS"
      WHERE substr("ORDERS"."SOURCE", 1, 6)=:Appeal
      GROUP BY "ORDERS"."ORDER_NUMBER",
               "ORDERS"."ORDER_TYPE",
               "ORDERS"."PAYMENT_METHOD",
               "ORDERS"."PAYMENT_FREQUENCY",
               "ORDERS"."SOURCE") "Query2"
   GROUP BY "Query2"."SOURCE") "Query3",
                               "SOURCES" "Sources"
WHERE "Query3"."SOURCE"="Sources"."SOURCE"
GROUP BY "Query3"."SOURCE",
         "Sources"."SOURCE_DESC"</CommandText>
      </Query>
