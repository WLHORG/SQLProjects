    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":Appeal">
            <Value>=Parameters!Appeal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT "financialtotals"."SOURCE" "SOURCE",
       "Sources"."SOURCE_DESC" "SOURCE_DESC",
       sum("financialtotals"."AMOUNT") "AMOUNT",
       sum("financialtotals"."donations") "donations"
FROM
  (SELECT "financial"."SOURCE" "SOURCE",
          sum("financial"."AMOUNT") "AMOUNT",
          sum("financial"."donations") "donations"
   FROM
     (SELECT "FromFH"."BATCH_NUMBER" "BATCH_NUMBER",
             "FromFH"."TRANSACTION_NUMBER" "TRANSACTION_NUMBER",
             "FromFH"."CONTACT_NUMBER" "CONTACT_NUMBER",
             sum("FromFHD"."AMOUNT") "AMOUNT",
             "FromFHD"."SOURCE" "SOURCE",
             count("FromFHD"."SOURCE") "donations"
      FROM
        (SELECT DISTINCT "FINANCIAL_HISTORY"."BATCH_NUMBER" "BATCH_NUMBER",
                         "FINANCIAL_HISTORY"."TRANSACTION_NUMBER" "TRANSACTION_NUMBER",
                         "FINANCIAL_HISTORY"."CONTACT_NUMBER" "CONTACT_NUMBER"
         FROM "FINANCIAL_HISTORY" "FINANCIAL_HISTORY") "FromFH",
        (SELECT "FINANCIAL_HISTORY_DETAILS"."BATCH_NUMBER" "BATCH_NUMBER",
                "FINANCIAL_HISTORY_DETAILS"."TRANSACTION_NUMBER" "TRANSACTION_NUMBER",
                "FINANCIAL_HISTORY_DETAILS"."LINE_NUMBER" "LINE_NUMBER",
                sum("FINANCIAL_HISTORY_DETAILS"."AMOUNT") "AMOUNT",
                "FINANCIAL_HISTORY_DETAILS"."SOURCE" "SOURCE"
         FROM "FINANCIAL_HISTORY_DETAILS" "FINANCIAL_HISTORY_DETAILS"
         GROUP BY "FINANCIAL_HISTORY_DETAILS"."BATCH_NUMBER",
                  "FINANCIAL_HISTORY_DETAILS"."TRANSACTION_NUMBER",
                  "FINANCIAL_HISTORY_DETAILS"."LINE_NUMBER",
                  "FINANCIAL_HISTORY_DETAILS"."SOURCE") "FromFHD"
      WHERE "FromFH"."BATCH_NUMBER"="FromFHD"."BATCH_NUMBER"
        AND "FromFH"."TRANSACTION_NUMBER"="FromFHD"."TRANSACTION_NUMBER"
      GROUP BY "FromFH"."BATCH_NUMBER",
               "FromFH"."TRANSACTION_NUMBER",
               "FromFH"."CONTACT_NUMBER",
               "FromFHD"."SOURCE" HAVING substr("FromFHD"."SOURCE", 1, 6)=:Appeal) "financial"
   GROUP BY "financial"."SOURCE") "financialtotals",
                                  "SOURCES" "Sources"
WHERE "Sources"."SOURCE"="financialtotals"."SOURCE"
GROUP BY "financialtotals"."SOURCE",
         "Sources"."SOURCE_DESC"</CommandText>
      </Query>
