  <DataSets>
    <DataSet Name="Potential_Gift_Aid_Claim">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @cv varchar(2) = (SELECT CONFIG_VALUE FROM [CISLIVE]..[CARE_ADMIN].[CONFIG] WHERE CONFIG_NAME = 'ga_tax_claim_exclude_gone_away')
DECLARE @ga varchar(2) = (SELECT GONE_AWAY_STATUS FROM [CISLIVE]..[CARE_ADMIN].[CONTACT_CONTROLS]);

with potential_gift_aid as

(select dlu.contact_number
,fh.address_number 		
,gad.source
,s.source_desc
,dlu.batch_number
,dlu.transaction_number
,dlu.line_number
,rev.was_batch_number
,rev.was_transaction_number
,rev.was_line_number
,dlu.cd_number
,dlu.declaration_or_covenant_number
,dlu.net_amount 
,fhd.amount 
,gad.confirmed_on 
,fh.transaction_date
,p.nominal_account
,fhd.rate
,fhd.product
,p.product_desc 
,fh.contact_number as fh_contact_number
,gad.declaration_type
,gad.[start_date]
,gad.end_date
,gad.method
from cis.declaration_lines_unclaimed dlu 
inner join cis.gift_aid_declarations gad on dlu.cd_number = gad.declaration_number
inner join cis.financial_history fh on dlu.batch_number = fh.batch_number 
							and dlu.transaction_number = fh.transaction_number 
inner join cis.financial_history_details fhd on dlu.batch_number = fhd.batch_number 
									and dlu.transaction_number = fhd.transaction_number 
									and fhd.line_number = dlu.line_number
inner join cis.products p on fhd.product = p.product 
inner join cis.sources s on gad.source = s.source
left join cis.reversals rev on rev.batch_number = fhd.batch_number 
							and rev.transaction_number = fhd.transaction_number 
							and fhd.line_number = rev.line_number 
where transaction_date &gt;= @startdate
and transaction_date &lt;= @enddate
and (method = 'w' 
		or (method = 'o' 
			and confirmed_on is not null 
			and confirmed_on  &lt;= dateadd(dd,-30,cast(getdate() as date)) )) 
and declaration_or_covenant_number = 'd' )

----------------------Output-----------------------------------------------------------------------
select pga2.contact_number
, pga2.label_name as donor
, pga2.[status]
, pga2.address_number
, pga2.address_usages
, pga2.address_line1
, pga2.postcode
, stuff((select ', ' + av.activity_value_desc
			from cis.contact_categories cc
			inner join cis.activity_values as av on CC.activity=av.activity 
						and cc.activity_value=av.activity_value
			where cc.activity='GAT'
			and cc.contact_number=pga2.contact_number
			and cc.source = pga2.source
			and getdate() between valid_from and valid_to
			for xml path ('')), 1, 1, '')  as gift_aid_type_desc
, pga2.[source]
, pga2.source_desc
, cast(pga2.batch_number as varchar(10)) + '/' + cast(pga2.transaction_number as varchar(10)) + '/' + cast(pga2.line_number as varchar(10)) as transaction_reference
, cast(pga2.was_batch_number as varchar(10)) + '/' + cast(pga2.was_transaction_number as varchar(10)) + '/' + cast(pga2.was_line_number as varchar(10)) as reversal_was_transaction_reference
, cast(pga2.cd_number as varchar(10)) + '/' + declaration_or_covenant_number as gift_aid_reference
, pga2.net_amount as net_donations_received
, pga2.amount /*as donations_received*/ as Donation_Breakdown
, cast(pga2.net_amount * 25 / 100 as numeric(9,2)) as gift_aid_claim_amount
, cast(pga2.confirmed_on as date) as gift_aid_confirmed_date
, cast(pga2.transaction_date as date) as date_of_payment
, left(pga2.nominal_account, 5) as account_code
, right(pga2.nominal_account, 4) as activity_code
, pga2.rate
, pga2.product
, pga2.product_desc as product_description
, pga2.fh_contact_number
, case when pga2.declaration_type = 'A' then 'Both'
		when pga2.declaration_type = 'D' then 'Donation'
		when pga2.declaration_type = 'M' then 'Member' end as declaration_type_desc		
,cast(pga2.[start_date] as date) as [start_date]
,cast(pga2.end_date as date) as [end_date]
,case when pga2.method = 'O' then 'Oral' 
		when pga2.method = 'W' then 'Written' end as gift_aid_type
from (select distinct pga.contact_number
					, c.label_name 
					, c.[status]
					, pga.address_number
					, stuff((select ', ' + au.address_usage_desc
							  from cis.contact_address_usages cau
							  join cis.address_usages as au on cau.address_usage=au.address_usage
							  where cau.contact_number = vu.contact_number 
							  and cau.address_number = vu.address_number
							  for xml path ('')), 1, 1, '') as address_usages
					, a.address_line1
					, a.postcode
					, pga.[source]
					, pga.source_desc
					, pga.batch_number
					, pga.transaction_number
					, pga.line_number
					, pga.was_batch_number
					, pga.was_transaction_number
					, pga.was_line_number
					, pga.cd_number
					, pga.declaration_or_covenant_number
					, pga.net_amount
					, pga.amount
					, pga.confirmed_on
					, pga.transaction_date
					, pga.nominal_account
					, pga.rate
					, pga.product
					, pga.product_desc
					, pga.fh_contact_number
					, pga.declaration_type
					, pga.[start_date]
					, pga.end_date
					, pga.method
		from potential_gift_aid as pga
		inner join cis.contacts c on pga.contact_number = c.contact_number 
		inner join cis.addresses a on pga.address_number = a.address_number	
		inner join custom.cis_universefile_output as vu on pga.contact_number=vu.contact_number
		where (c.[status] is null 
				or (c.[status] is not null 
				and c.[status] not in (case when @cv = 'y' 
												then @ga else '' 
										end)))) as pga2</CommandText>
