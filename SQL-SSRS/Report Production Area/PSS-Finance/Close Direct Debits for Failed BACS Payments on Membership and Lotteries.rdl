  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":batch_number">
            <Value>=Parameters!batch_number.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare
k_name constant varchar(10) :='ARUDD';
k_cancel_date constant contact_journals.journal_time%type :=sysdate;
k_cancel_reason constant varchar(4):='RP';
v_contact_number orders.contact_number%type;
v_address_number orders.address_number%type;
v_direct_debit_number direct_debits.direct_debit_number%type;
v_membership_number members.membership_number%type;
v_contact_journal_number contact_journals.contact_journal_number%type;

    TYPE t_tbl_ord_no IS TABLE OF orders.order_number%TYPE
       INDEX BY PLS_INTEGER;

tbl_ord_no              t_tbl_ord_no;

CURSOR MY_CURSOR  
  is
select o.order_number
from order_payment_history oph 
	join orders o on o.order_number = oph.order_number 
	join direct_debits dd on dd.order_number = o.order_number 
	join reversals r on r.batch_number = oph.batch_number and r.transaction_number = oph.transaction_number and r.line_number = oph.line_number
	join batches b on b.batch_number = oph.batch_number
	join bacs_amendments ba on ba.advice_reference = dd.reference and b.batch_date = ba.amended_on
where oph.batch_number = :batch_number
	and dd.cancelled_on is null 
	and ba.bacs_advice_reason = '0' 
	and dd.bank_account IN( 'LDD','MDD' );

BEGIN

OPEN MY_CURSOR;
FETCH MY_CURSOR bulk collect into tbl_ord_no;
CLOSE MY_CURSOR;

IF tbl_ord_no.COUNT &gt; 0 THEN
    FOR i in tbl_ord_no.FIRST .. tbl_ord_no.LAST LOOP
    
      select distinct o.contact_number,
                        o.address_number,
                        dd.direct_debit_number ,
                        m.membership_number
        into v_contact_number,
            v_address_number,
            v_direct_debit_number,
            v_membership_number
        from orders o inner JOIN direct_debits dd
                            on o.order_number= dd.order_number
                            and o.contact_number=dd.contact_number
                            and dd.cancelled_by is null
                      left join members m
                            on o.order_number= m.order_number
                            and o.contact_number= m.contact_number
                            and m.cancelled_by is null
        where o.order_number=tbl_ord_no(i);
  
        UPDATE DIRECT_DEBITS 
        SET cancellation_reason = k_cancel_reason
          ,  cancelled_on = k_cancel_date
          ,  cancelled_by = k_name
		  ,  amended_on = k_cancel_date
          ,  amended_by = k_name
		  ,  auddis_cancel_notified = k_cancel_date
        WHERE order_number = tbl_ord_no(i)
        AND cancellation_reason IS NULL;
        
        UPDATE ORDER_PAYMENT_SCHEDULE 
        SET amended_by = k_name
          ,  amended_on = k_cancel_date
          ,  scheduled_payment_status = 'C'
        WHERE order_number = tbl_ord_no(i)
         AND amount_outstanding &gt; 0 
         AND scheduled_payment_status NOT IN ( 'F', 'S', 'A' );
      
        UPDATE MEMBERS 
        SET cancellation_reason = k_cancel_reason
          ,  cancelled_on = k_cancel_date
          ,  cancelled_by = k_name
        WHERE order_number = tbl_ord_no(i) 
         AND cancellation_reason IS NULL;
      
        UPDATE CONTACT_CATEGORIES 
        SET valid_to = k_cancel_date
          ,  amended_on = k_cancel_date
        WHERE contact_number = (select contact_number from members where order_number =tbl_ord_no(i) and rownum=1)
         AND activity = 'LAY';
      
        DELETE
        FROM ORDER_DETAIL_ARREARS
        WHERE order_number = tbl_ord_no(i) ;
      
        UPDATE ORDERS 
        SET cancellation_reason = k_cancel_reason
          ,  cancelled_on = k_cancel_date
          ,  cancelled_by = k_name
          ,  bankers_order = 'N'
          ,  direct_debit = 'C'
          ,  credit_card = 'N'
          ,  covenant = 'N'
        WHERE order_number = tbl_ord_no(i)
         AND cancellation_reason IS NULL;
      
      select max(contact_journal_number)+1
       into v_contact_journal_number
       from contact_journals;
      
      INSERT INTO contact_journals (contact_journal_number, journal_type, journal_time, journal_by, operation, contact_number, address_number, select_1) 
      VALUES (v_contact_journal_number, 'DD', k_cancel_date, k_name, 'cancel', v_contact_number, v_address_number, v_direct_debit_number);
      
      select max(contact_journal_number)+1
       into v_contact_journal_number
       from contact_journals;
      
      INSERT INTO contact_journals (contact_journal_number, journal_type, journal_time, journal_by, operation, contact_number, address_number, select_1) 
      VALUES (v_contact_journal_number, 'PPPS', k_cancel_date, k_name, 'update', v_contact_number, v_address_number, tbl_ord_no(i));
      
      select max(contact_journal_number)+1
       into v_contact_journal_number
       from contact_journals;
       
       INSERT INTO contact_journals (contact_journal_number, journal_type, journal_time, journal_by, operation, contact_number, address_number, select_1) 
       VALUES (v_contact_journal_number, 'MEM',  k_cancel_date, k_name, 'cancel', v_contact_number, v_address_number, v_membership_number);
       
      select max(contact_journal_number)+1
       into v_contact_journal_number
       from contact_journals;
       
       INSERT INTO contact_journals (contact_journal_number, journal_type, journal_time, journal_by, operation, contact_number, address_number, select_1)
       VALUES (v_contact_journal_number, 'PP',  k_cancel_date, k_name, 'cancel', v_contact_number, v_address_number, tbl_ord_no(i));

    END LOOP;
END IF;

 update control_numbers   
 set control_number=(select max(contact_journal_number)
                          from contact_journals)
 where control_number_type='CJ';

END;



 </CommandText>
      </Query>
