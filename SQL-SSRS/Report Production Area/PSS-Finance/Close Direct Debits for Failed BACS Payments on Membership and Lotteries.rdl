    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select '' || to_char(batch_number) || ' [' || substr(to_char(amended_on),1,10) || '] for ' || bank_account  label , batch_number 
from batches 
where batch_type = 'FA' 
	and amended_on &gt; sysdate - interval '60' day  
	and bank_account IN ('MDD','LDD')
order by amended_on desc, batch_number desc</CommandText>
      </Query>
