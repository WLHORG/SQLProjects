  <DataSets>
    <DataSet Name="MainSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ED">
            <Value>=Parameters!ED.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select i.sales_ledger_account,
		i.contact_number,
        i.invoice_number,
        i.invoice_date,
        bt.transaction_date,
        bta.amount,
        --i.amount_paid,
		c.contact_type,
        c.label_name,
        c.salutation,
        c.title,
        c.forenames,
        c.surname,
        a.address_line1,
        a.address_line2,
        a.address_line3,
        a.address_line4,
        a.postcode,
        a.town,
        co.number email,
        datediff(dy,i.invoice_date,@ED) days_os,
        SUM(bta.amount)-max(i.amount_paid) balance
from cis.invoices i inner join cis.batch_transaction_analysis bta
					  on i.batch_number=bta.batch_number
					  and i.transaction_number=bta.transaction_number
					  and i.invoice_pay_status in ('N','P')
					  and i.invoice_date&lt;=@ED
					  and not(bta.amount is null)
                 inner join cis.batch_transactions bt
					  on bta.batch_number=bt.batch_number
					  and bta.transaction_number=bt.transaction_number
				 inner join cis.contacts c
					  on c.contact_number=i.contact_number
				inner join cis.addresses a
						on a.address_number=(select  case when (count(distinct cau.address_number)&gt;0) and max(c1.contact_type)='C' then MAX(cau.address_number)
											             when (count(distinct oau.address_number)&gt;0) and max(c1.contact_type)='O' then MAX(oau.address_number)
													else MAX(c1.address_number)
											end
											from cis.contacts c1 left join cis.contact_addresses ca1
															  on c1.contact_number=ca1.contact_number
															  and ca1.historical='N'
														  left join cis.contact_address_usages cau 
															  on cau.address_number=ca1.address_number
															  and cau.address_usage='IA'
														 left join cis.organisation_addresses oa1
															  on c1.contact_number=oa1.organisation_number
															  and oa1.historical='N'
														  left join cis.organisation_address_usages oau 
															  on oau.address_number=oa1.address_number
															  and oau.address_usage='IA'
											where c1.contact_number=i.contact_number)
				left join cis.communications co
						on co.contact_number=i.contact_number
						and co.communication_number=(select case when MAX(ccu.communication_number) is not null then max(ccu.communication_number)
																else max(co1.communication_number)
															end
													from cis.communications co1 left join cis.contact_communication_usages ccu
																				on co1.communication_number=ccu.communication_number
																				and ccu.communication_usage='IAP'
													where (valid_to is null or valid_to &gt; getdate())
															and device in ('EM','EC','WR')
															and is_active = 'Y'
															and co1.contact_number=co.contact_number)
group by i.sales_ledger_account,
			i.contact_number,
			i.invoice_number,
			bt.transaction_date,
			i.invoice_date,
			bta.amount,
			--i.amount_paid,
			c.contact_type,
			c.label_name,
			c.salutation,
			c.title,
			c.forenames,
			c.surname,
			a.address_line1,
			a.address_line2,
			a.address_line3,
			a.address_line4,
			a.postcode,
			a.town,
			co.number
having (SUM(bta.amount)-max(i.amount_paid))&gt;0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
