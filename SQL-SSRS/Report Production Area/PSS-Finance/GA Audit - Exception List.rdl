    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="exception">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT c.contact_number, 
                c.label_name, 
                a.address, 
                a.town, 
                a.postcode, 
                --a.address_type,cau.address_usage,au.address_usage_desc, 
                gad.method, 
                gad.declaration_date, 
                gad.start_date, 
                gad.created_on, 
                gad.amended_on, 
                gad.created_by, 
                CASE 
				  WHEN a.town = 'UNKNOWN' THEN 'Unknown Address'
				  WHEN a.country NOT IN ('UK','NI','GBA','GBG','GBJ','GBM','GBZ') THEN 'Overseas Address'
                  WHEN A.address_type = 'O' THEN 
                  'Using linked organisational address as default address' 
                  WHEN cau.address_usage NOT IN ( 'mc', 'mh', 'h2' ) THEN 
                  au.address_usage_desc 
                  WHEN ( gad.method = 'O' 
                         AND gad.confirmed_on IS NULL ) THEN 
                  'Oral GAD without Confirmation' 
                  ELSE '' 
                END REASON, 
				CASE 
					WHEN cl.contact_number IS NOT NULL THEN 'W/ payments'
					ELSE 'WO/ payments'
				END RecentPaymentExist
FROM   cis.contacts c 
       INNER JOIN cis.contact_addresses ca 
               ON c.contact_number = ca.contact_number 
       INNER JOIN cis.addresses a 
               ON ca.address_number = a.address_number 
       INNER JOIN cis.gift_aid_declarations gad 
               ON c.contact_number = gad.contact_number 
       LEFT JOIN cis.contact_address_usages cau 
              ON cau.contact_number = c.contact_number 
                 AND cau.address_number = cau.address_number 
       LEFT JOIN cis.address_usages au 
              ON au.address_usage = cau.address_usage 
	   LEFT JOIN (SELECT distinct contact_number 
					FROM cis.financial_history 
					WHERE posted &gt; getdate() - 365*4) cl 
			  ON cl.contact_number = c.contact_number
WHERE  Getdate() BETWEEN start_date AND Isnull(end_date, '2999-01-01') 
		-- ACTIVE GADs
       AND c.address_number - a.address_number = 0 
		-- DEFAULT ADDRESS
       AND ( a.country NOT IN ('UK','NI','GBA','GBG','GBJ','GBM','GBZ') OR a.town = 'UNKNOWN' OR A.address_type = 'O' 
              OR cau.address_usage NOT IN ( 'mc', 'mh', 'h2' ) 
              OR ( gad.method = 'O' 
                   AND gad.confirmed_on IS NULL ) ) 
		-- ERRORED GADs
ORDER  BY reason </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
