        <CommandText>--declare @ed datetime =  DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()), 0);

if object_id('tempdb..#NewCodesDistinct') is not null drop table #NewCodesDistinct;
select A.[source]
, A.new_source
, A.source_counter
, A.new_product
, A.new_cost_centre
, A.new_programme
, A.product  
into #NewCodesDistinct
from (select distinct s.[source]
			, s.new_source
			, row_number() over (partition by s.[source] order by s.[source]) as source_counter
			, s.new_product
			, s.new_cost_centre
			, s.new_programme
			, p.product  
		from JonathanL.New2015_Sources s
		left join cis.duk_new2015_products p on s.new_product = p.new_product) as A
where source_counter = 1 
and new_product is null

insert into #NewCodesDistinct
([source]
, new_source
, source_counter
, new_product
, new_cost_centre
, new_programme
, product)
select A.[source]
, A.new_source
, A.source_counter
, A.new_product
, A.new_cost_centre
, A.new_programme
, A.product  
from (select distinct s.[source]
			, s.new_source
			, row_number() over (partition by s.[source] order by s.[source]) as source_counter
			, s.new_product
			, s.new_cost_centre
			, s.new_programme
			, p.product  
		from JonathanL.New2015_Sources s
		left join cis.duk_new2015_products p on s.new_product = p.new_product) as A			
where source_counter = 1 
and new_product is not null
and [source] not in (select [source] 
						from #NewCodesDistinct)

select distinct p.company,
		--r.nominal_account_suffix reg_anal_code,
		--bta.[source],
		ncc.cost_centre,
		ncc.cost_centre_desc,
		new.new_programme as programme,
		prog.programme_desc,
		i.contact_number,
		c.label_name,
		i.invoice_number,
		right(p.nominal_account,4) activity_code,
		i.invoice_date,
		--fh.transaction_date,
		i.invoice_pay_status,
		datediff(dy,i.invoice_date,@ed) days_os,
		SUM(bta.amount)-max(i.amount_paid) balance
from cis.invoices i
inner join cis.contacts c on i.contact_number=c.contact_number
inner join cis.batch_transaction_analysis bta
					on i.batch_number=bta.batch_number
					and i.transaction_number=bta.transaction_number
					and i.invoice_pay_status in ('N','P')
					and i.invoice_date&lt;=@ed
inner join cis.products p on p.product=bta.product
inner join cis.rates r on r.rate=bta.rate 
						and r.product=bta.product
						and not(bta.amount is null)
--left join cis.invoice_payment_history iph on i.invoice_number=iph.invoice_number
--left join cis.financial_history fh on fh.batch_number=iph.batch_number
									--and fh.transaction_number=iph.transaction_number
--left join cis.financial_history_details fhd on fh.batch_number=fhd.batch_number 
--											and fh.transaction_number=fhd.transaction_number
left join #NewCodesDistinct as new on bta.[source] = new.[source]
left join cis.duk_cost_centres as ncc on new.new_cost_centre = ncc.cost_centre
left join cis.duk_programmes as prog on new.new_programme=prog.programme
---where (fh.transaction_date is null 
		--or fh.transaction_date&gt;=@ed)
group by p.company,
			--r.nominal_account_suffix,
			--bta.[source],
			ncc.cost_centre,
			ncc.cost_centre_desc,
			new.new_programme,
			prog.programme_desc,
			i.contact_number,
			c.label_name,
			i.invoice_number,
			right(p.nominal_account,4),
			i.invoice_date,
			--fh.transaction_date,
			i.invoice_pay_status
having (SUM(bta.amount)-max(amount_paid))&gt;0
order by i.invoice_number;

