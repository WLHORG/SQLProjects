        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT c.contact_number 
	   ,c.label_name
	   ,cc.activity   --catcode
	   ,cc.activity_value  --catdescript
	   ,cc.[source]
	   ,s.source_desc
	   ,cc.valid_from
	   ,cc.valid_to
from cis.contact_categories cc
INNER JOIN cis.contacts c ON c.contact_number = cc.contact_number
INNER JOIN cis.sources s ON c.source = s.source
where activity in ('LE','LP', 'LPI', 'LEGCON')
and valid_from between DATEADD("d", -31, getdate()) AND  getdate()</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
