      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Data Flow Task\ODBC Source"
              componentClassID="Attunity.SSISODBCSrc"
              contactInfo="ODBC Source;Connector for Open Database Connectivity (ODBC) by Attunity; Attunity Ltd.; All Rights Reserved; http://www.attunity.com;2"
              description="ODBC Source"
              name="ODBC Source"
              usesDispositions="true"
              version="2">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand">SELECT		 fh.transaction_date
			, fhd.[source]
			, ISNULL((SELECT o.label_name from  [dbo].[contacts] o where contact_type = 'O' AND o.contact_number = c.contact_number), 'N/A')  as OrgName
			, ISNULL((SELECT o.label_name from  [dbo].[contacts] o where contact_type = 'C' AND o.contact_number = c.contact_number), 'N/A')  as ContactName
			, a.[address] as [address]
			, (SELECT CASE WHEN ad.address LIKE '%Unknown%' THEN 'UNKNOWN' WHEN ad.address LIKE  '%Unmatched S/O%'  THEN 'UNKNOWN' WHEN ad.country NOT IN ('UK', 'NI') THEN 'OVERSEAS'  ELSE REPLACE(ad.postcode, ' ', '') END as postcode from  [dbo].[addresses] ad where ad.[address_number] =  a.[address_number])  as postcode			
			, fhd.batch_number
			, fhd.transaction_number
			, fhd.line_number
			, fh.transaction_type
			, fh.payment_method
			, oph.order_number
			, ISNULL(fhd.product, 0) as product
			, fhd.quantity
			, SUM(fhd.amount) as amount
			, fhd.vat_amount
			, GETDATE() as DateLoad
from financial_history as fh
inner join financial_history_details as fhd on fh.batch_number=fhd.batch_number and fh.transaction_number=fhd.transaction_number
left join (select batch_number
                           , transaction_number
                           , line_number
                           , order_number
                     from order_payment_history) as oph on fhd.batch_number=oph.batch_number and fhd.transaction_number=oph.transaction_number
                                                                                                 and fhd.line_number=oph.line_number
INNER JOIN contacts c ON fh.contact_number = c.contact_number
INNER JOIN addresses a ON fh.address_number = a.address_number 
WHERE fh.transaction_date between '2014-01-01' AND '2014-01-31'
--AND c.label_name NOT IN (SELECT label_name FROM CONTACTS) 
GROUP BY    fh.transaction_date 
			,fhd.batch_number  
			,fhd.transaction_number
			,fhd.line_number
			,c.label_name
			,a.[address]
			,fh.batch_number
			,fh.transaction_number
			,fh.contact_number
			,fh.transaction_date
			,fh.transaction_type
			,fh.payment_method
			,fhd.product
			,fhd.rate
			,fhd.[source]
			,fhd.quantity
			,oph.order_number
			,fhd.vat_amount
			,c.contact_number
			,a.postcode
			,a.town
			,a.[address_number]</property>
